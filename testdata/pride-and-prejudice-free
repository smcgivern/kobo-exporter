                <section class="fill-in-review-row">
                    <label for="writeReviewInput" class="sub-section-title write-review-title">Review *</label>

                    <div class="how-to-container">
                        <button class="write-review-how-to" aria-expanded="false" aria-describedby="howToTips">
                            How to write a great review
                            <span class="triangle-with-shadow"></span>
                        </button>
                        <div id="howToTips" class="how-to-tips" role="tooltip" aria-live="polite">
                            <h5 class="how-to-dos">Do</h5>
                            <ul class="tips-listing">
                                <li>Say what you liked best and least</li>
                                <li>Describe the author&#39;s style</li>
                                <li>Explain the rating you gave</li>
                            </ul>

                            <h5 class="how-to-donts">Don&#39;t</h5>
                            <ul class="tips-listing">
                                <li>Use rude and profane language</li>
                                <li>Include any personal information</li>
                                <li>Mention spoilers or the book&#39;s price</li>
                                <li>Recap the plot</li>
                            </ul>
                        </div>
                    </div>

                    <textarea type="text" class="write-review-input" id="writeReviewInput"
                              maxlength="5000"
                              data-min-length="50"
                              placeholder="Tell us if you enjoyed the story. What was the author&#39;s style like? What did you like best and the least? Who would you recommend this book for?"
                              title="Tell us if you enjoyed the story. What was the author&#39;s style like? What did you like best and the least? Who would you recommend this book for?"
                              aria-required="true"></textarea>
                    <p class="review-character-counter">(<span id="counter">0</span>) <span translate="no">50</span> characters minimum</p>

                    <p class="error-message" id="error-message-reviewtextarea">The review must be at least <span translate="no">50</span> characters long.</p>
                </section>

                <!-- Give the review a title -->
                <section class="title-row">
                    <label for="titleInput" class="sub-section-title new-review-title">Title *</label>
                    <input id="titleInput" type="text" class="new-review-title-input" maxlength="50" placeholder="Briefly give your overall impression." title="Briefly give your overall impression." aria-required="true" />

                    <p class="error-message" id="error-message-title">The title should be at least <span translate="no">4</span> characters long.</p>
                </section>

